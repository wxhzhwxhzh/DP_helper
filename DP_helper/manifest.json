{
    "manifest_version": 3,
    "name": "骚神库DP助手",
    "version": "8.5",
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "storage",
        "activeTab",
        "scripting",
        "sidePanel"
    ],
    "icons": {
        "128": "img/saoshen2.png",
        "48": "img/saoshen2.png",
        "19": "img/saoshen2.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/jquery-3.7.1.min.js",
                "js/jquery-ui.js",
                "content.js",
                "tool.js"
            ],
            "css": [
                "content.css",
                "css/jquery-ui.css"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/inject.js",
                "*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "homepage_url": "https://gitee.com/Saossion/SaossionPage",
    "action": {
        "default_popup": "popup.html"
    },
    "devtools_page": "dev_tool.html",
    "background": {
        "service_worker": "background.js"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    
      
    "omnibox": { "keyword" : "sao" }
   
    


}